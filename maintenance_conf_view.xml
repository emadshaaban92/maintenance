<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record model="ir.ui.view" id="maintenance_service_type_tree_view">
			<field name="name">maintenance.service.type.tree</field>
			<field name="model">maintenance.service.type</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
			    <tree string="Service Types">
			        <field name="name"/>
			    </tree>
			</field>
		</record>
		
		<record model="ir.ui.view" id="maintenance_service_type_form_view">
			<field name="name">maintenance.service.type.form</field>
			<field name="model">maintenance.service.type</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
			    <form string="Service Types" version="7.0">
			        <group>
			            <field name="name"/>
			        </group>
			    </form>
			</field>
		</record>
		
		<record model="ir.actions.act_window" id="action_maintenance_service_type_form">
			<field name="name">Service Type</field>
			<field name="res_model">maintenance.service.type</field>
			<field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
		</record>
		
		

		<record model="ir.ui.view" id="maintenance_asset_type_tree_view">
			<field name="name">maintenance.asset.type.tree</field>
			<field name="model">maintenance.asset.type</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
			    <tree string="Asset Types">
			        <field name="name"/>
			    </tree>
			</field>
		</record>
		
		<record model="ir.ui.view" id="maintenance_asset_type_form_view">
			<field name="name">maintenance.asset.type.form</field>
			<field name="model">maintenance.asset.type</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
			    <form string="Asset Types" version="7.0">
			        <group>
			            <field name="name"/>
			        </group>
			    </form>
			</field>
		</record>
		
		<record model="ir.actions.act_window" id="action_maintenance_asset_type_form">
			<field name="name">Asset Type</field>
			<field name="res_model">maintenance.asset.type</field>
			<field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
		</record>
		
		

	</data>
</openerp>
